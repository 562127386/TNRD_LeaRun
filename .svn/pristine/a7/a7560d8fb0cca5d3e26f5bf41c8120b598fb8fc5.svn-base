function searchByYear(year) {
    if (year.text.trim() != "全部") {
        var param = { Date: year.text } || {};
    } else {
        var param = { Date: '' } || {};
    }
    $('#gridtable').jfGridSet('reload', { queryJson: JSON.stringify(param) });
}